<?xml version="1.0" ?>
	<autopackSetup name="Mycoplasma" version="1.2">
	<!-- inspired from David Goodsell Mycoplasma mycoides complete cell (3000A diameter minimum)-->
	<options EnviroOnly="False" _hackFreepts="False" _timer="False" 
            boundingBox="[[-1445.0, -1445.0, -2200.0], [1445.0, 1445.0, 2200.0]]" 
            cancelDialog="False" computeGridParams="True" freePtsUpdateThrehod="0.0" 
            gradients="['gX']" ingrLookForNeighbours="True" innerGridMethod="jordan" 
            largestProteinSize="100" overwritePlaceMethod="True" pickRandPt="True" 
            pickWeightedIngr="True" placeMethod="pandaBullet" 
            runTimeDisplay="False" saveResult="False" smallestProteinSize="100." 
            resultfile="/Users/ludo/DEV/autofill_svn/trunk/AutoFillClean/autoFillRecipeScripts/Mycoplasma/results/MycoplasmaPackResult.apr"
            use_gradient="True" version="1.0" windowsSize="100"/>
	<gradients>
    	<gradient description="" direction="[0.5, 0.5, 0.5]" mode="X" name="gX" 
                pick_mode="sub" radius="10.0" weight_mode="gauss"/>
	</gradients>
	<cytoplasme/>
	<organelle geom="http://autofill.googlecode.com/git/data/HIV/geoms/Envelope.dae" name="Envelope" rep="None" rep_file="None">
   </organelle>
	<organelle geom="http://autofill.googlecode.com/git/data/Mycoplasma/geoms/Mmycoides_PackingSurf_1_Med.dae" 
                name="MycoplasmaPackSurf_1_med" rep="MycoplasmaRepSurf_1_med" rep_file="http://autofill.googlecode.com/git/data/Mycoplasma/geoms/Mmycoides_RepSurf_1_Med.dae">
	<!--surface include="http://autofill.googlecode.com/git/data/SynapticVesicle/recipe/SynapticVesicle_ingredients.xml"-->
	<surface>
<!--17. ATP synthase-->
	<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/Ves203MeshsParent.xml">
        <overwrite principalVector="[0.0, 0.0, 1.0]"/>
	</ingredient>
<!--18. secretory proteins PDB OPM 2zjs-->
	<!--ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_2zjs.xml"/-->
<!--19. sodium pump PDB OPM 4av3-->
	<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_4av3.xml"/>
<!--20. zinc transporter PDB OPM 3j1z-->
	<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_3j1z.xml"/>
<!--21. magnesium transporter PDB OPM 4i0u-->
	<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_4i0u.xml">
        <overwrite principalVector="[0.0, 1.0, 0.0]"/>
	</ingredient>
<!--22. ABC transporter (different ABC transporters transport different types of molecules-ABC is short for "ATP-binding cassette") PDB OPM 1l7v-->
	<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1l7v.xml">
        <overwrite principalVector="[0.0, 0.0, 1.0]"/>
	</ingredient>

<!--24. lypoglycan (long carbohydrate chains connected to lipid in the membrane)--> 
		<ingredient Type="Grow" biased="0.5" randomd="False" 
                color="(1.0, 0.498, 0.314)" 
                cutoff_boundary="0.0" cutoff_surface="0.0" gradient="" 
                jitterMax="(1, 1, 1)" length="2350" marge="45.0" 
                molarity="1" name="hair" nbJitter="1" nbMol="0" 
                orientation="(1, 0, 0)" packingMode="random" packingPriority="-10000.0" 
                perturbAxisAmplitude="0.1" placeType="jitter" positions="[[[0, 0, 0]]]" 
                positions2="[[[50.0, 0.0, 0.0]]]" uLength ="50" principalVector="(0, 1, 0)" 
                radii="[[5.0]]" useLength="False" walkingMode="sphere" 
                constraintMarge="False"/> 
    </surface>
	<!--interior include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/GenerycCytoplasm_ingredients.xml"-->
	<interior>
<!--1. DNA 1500kb->4119790A-->
       	<ingredient Type="Grow" biased="0.5" randomd="False" 
                color="(1.0, 0.498, 0.314)" 
                cutoff_boundary="55.0" cutoff_surface="25.0" gradient="" 
                jitterMax="(1, 1, 1)" length="4119790" marge="45.0" 
                molarity="0" name="DNA" nbJitter="1" nbMol="1" 
                orientation="(1, 0, 0)" packingMode="random" packingPriority="-10000.0" 
                perturbAxisAmplitude="0.1" placeType="jitter" positions="[[[0, 0, 0]]]" 
                positions2="[[[100.0, 0.0, 0.0]]]" uLength ="100" principalVector="(0, 1, 0)" 
                radii="[[10.0]]" useLength="False" walkingMode="sphere" 
                constraintMarge="False"/> 
<!--2. DNA polymerase PDB 1kln-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1kln.xml"/>
<!--3. single-stranded-DNA binding protein (protects single-stranded portions during replication) pdb 3AFP-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_3afp.xml"/>
<!--4. RNA polymerase pdb 4KMU-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_4kmu.xml"/>
<!--5. messenger RNA 2/1000A^3-->
       	<ingredient Type="Grow" biased="0.5" randomd="False" 
                color="(1.0, 0.498, 0.314)" 
                cutoff_boundary="55.0" cutoff_surface="25.0" gradient="" 
                jitterMax="(1, 1, 1)" length="1000" marge="80.0" 
                molarity="2e-09" name="mRNA" nbJitter="1" nbMol="0" 
                orientation="(1, 0, 0)" packingMode="random" packingPriority="-10000.0" 
                perturbAxisAmplitude="0.1" placeType="jitter" positions="[[[0, 0, 0]]]" 
                positions2="[[[100.0, 0.0, 0.0]]]" uLength ="100" principalVector="(0, 1, 0)" 
                radii="[[10.0]]" useLength="False" walkingMode="sphere" 
                constraintMarge="False"/> 

<!--6. ribosome pdb 4KBT 30/1000A^3-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_4kbt.xml"/>
<!--7. transfer RNA (in pink) 4tna 174/1000A^3 and elongation factor Tu (in blue)-->
<!--8. elongation factor Tu and Ts 60/1000A^3 20 alone-->
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/onemer1TTT_prot.xml"/>
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/onemer1TTT_rna.xml"/>
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/onemer1TTT.xml"/>
<!--9. elongation factor G pdb 1EFG  1dar 20-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1efg.xml"/>
<!--10. aminoacyl-tRNA synthetases 30-->
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/tetramer1ASY.xml"/>
<!--11. topoisomerases pdb 1A36-->
		<!--ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1a36_BC.xml"/>
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1a36_A.xml"/-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1a36.xml"/>
<!--12. Rec system for DNA repair: a) RecA pdb 1REA, b) RecBC pdb 1W36-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1rea.xml"/>
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1w36.xml"/>
<!--13. chaperonin GroEL (helps folding of new proteins) pdb 1AON-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1aon.xml"/>
<!--14. proteasome ClpA (destroys old proteins) pdb 1R6B-->
		<ingredient include="http://autofill.googlecode.com/git/data/Mycoplasma/recipe/ingredients/CoarseMS_1r6b.xml"/>
<!--15. glycolytic enymes 200-->
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/tetramer1PFK.xml"/>
<!--16. pyruvate dehydrogenase complex 1 -->
		<ingredient include="http://autofill.googlecode.com/git/data/GenericCytoplasm/recipe/ingredients/tetramer1A49.xml"/>
	</interior>
	</organelle>
	</autopackSetup>
