<?xml version="1.0" ?>
<AutoFillSetup name="HIV" recipeName=name>
    <autoPACK_FilePaths include="http://autofill.googlecode.com/svn/autoPACK_filePaths.xml"/>
    
        <?ignore autoPACK_FilePaths contents on Feb 24 2013 using this for quick reference to cutAndPaste filepaths
            <autoPACK_FilePaths name="autoPACK_FilePaths">
                <paths
                    autoPackDataBasePath="http://autofill.googlecode.com/svn/dataRight/"
                    localDataBase=""
                        recipesData=autoPackDataBasePath+recipeName+"/"
                        resultsAprPath   = recipesData+"Results/autoPACK_ResultFiles/"
                        recipesPath         = recipesData+"Recipes/HybridFiles/"
                        componentsPath      = recipesData+"Components/"
                            containersFilePath      = componentsPath+"HybridFiles/Containers/"
                            ingredientsFilePath     = componentsPath+"HybridFiles/Ingredients/"
                            representationsFilePath = componentsPath+"Representations/"
                                containerRepresentationsPath    = representationsFilePath+"Containers/"
                                ingredientRepresentationsPath   = representationsFilePath+"Ingredients/"
                            componentsDataPath  = componentsPath+"Data/"
                                componentsBioUnitMatricesPath   = componentsDataPath+"BioUnitMatrices/"
                                componentsCollisionTreesPath    = componentsDataPath+"CollisionTrees/"
                                componentsOtherDataPath         = componentsDataPath+"OtherData/"
                                componentsPackingSurfacesPath   = componentsDataPath+"PackingSurfaces/"
                                componentsPDBsPath              = componentsDataPath+"PDBs/"
                                componentsVolumeMapsPath        = componentsDataPath+"VolumeMaps/"
                />
            </autoPACK_FilePaths>
        ?>


	<options EnviroOnly="False"
        _hackFreepts="False"
        _timer="False"
        boundingBox="[[-693.41300000000001, -651.68600000000004, -735.077], [653.65499999999997, 664.005, 694.65200000000004]]"
        cancelDialog="False"
        computeGridParams="True"
        freePtsUpdateThrehod="0.15"
        gradients="['gradRadSPIKE', 'gradRadMA']"
        ingrLookForNeighbours="False"
        innerGridMethod="bhtree"
        largestProteinSize="1.0"
        overwritePlaceMethod="False"
        pickRandPt="True"
        pickWeightedIngr="True"
        placeMethod="jitter"
        resultfile=resultsAprPath+"HIVresult_1_4chimera3.apr"
        resultFileDefault=resultfile
        runTimeDisplay="0"
        saveResult="False"
        smallestProteinSize="50"
        use_gradient="True"
        version="1.0"
        windowsSize="50.0"
    />
	<gradients>
        <gradient description="" direction="[-480.0, -480.0, 0.0]" mode="radial" name="gradRadSPIKE" pick_mode="rnd" radius="1380.0" weight_mode="linear"/>
        <gradient description="" direction="[480.0, 480.0, 0.0]" mode="radial" name="gradRadMA" pick_mode="rnd" radius="1380.0" weight_mode="linear"/>
